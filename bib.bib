}

@Article{Gruenewald2015,
  Title                    = {Keep it simple: time-of-use tariffs in high-wind scenarios},
  Author                   = {Gr\"{u}newald, P. and McKenna, E. and Thomson, M.},
  Journal                  = {IET Renewable Power Generation},
  Year                     = {2015},
  Number                   = {2},
  Pages                    = {176--183},
  Volume                   = {9},

  __markedentry            = {[gpod-xps:6]},
  Abstract                 = {Price signals have been suggested to bring about greater demand side flexibility and thus support the integration of variable sources of energy, such as wind. A conflict exists between keeping these signals simple for consumers, while making responses appropriate for increasingly complex supply-demand balancing dynamics in future. This study reviews some of the demand responses observed in time-of-use (ToU) tariff trials and assesses their effectiveness in scenarios with higher levels of wind. The authors simulate wholesale real-time prices for high-wind scenarios as a benchmark tariff. Simple tariff structures are compared against real-time prices for the extent to which they can `nudge' demand in the `right direction'. They present results which suggest that even in high-wind scenarios, simple ToU tariffs could have a beneficial effect on overall system costs. The load shifting and reduction behaviour observed under ToU trials could lower energy costs by between 4 and 6\% without the need for complex price signals.},
  Doi                      = {10.1049/iet-rpg.2014.0031},
  File                     = {IEEE Xplore Abstract Record:/home/gpod-xps/.mozilla/firefox/5xitfka2.default/zotero/storage/V7BVDEU3/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/gpod-xps/.mozilla/firefox/5xitfka2.default/zotero/storage/E3ZUGUEG/Grünewald et al. - 2015 - Keep it simple time-of-use tariffs in high-wind s.pdf:application/pdf},
  ISSN                     = {1752-1416},
  Keywords                 = {demand response, demand side flexibility, demand side management, energy source integration, high-wind scenario, load reduction behaviour, load shifting behaviour, supply-demand balancing dynamic, tariffs, time-of-use tariff, ToU tariff, wind},
